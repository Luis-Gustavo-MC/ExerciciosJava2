import java.util.Scanner;



public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int horaI,horaF,x;
		System.out.println("Escreva a hora inicial e a hora final: ");
		horaI = sc.nextInt();
		horaF = sc.nextInt();
		if(horaI<horaF) {
			x = horaF - horaI;
		}else {
			x = 24 - horaI + horaF;
		}
		System.out.println("O jogo durou: " + x );
		sc.close();
	}
}
